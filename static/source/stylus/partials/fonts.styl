@font-face {
  font-family: 'als_rublregular';
  src: url('../fonts/rouble-webfont.eot');
  src: url('../fonts/rouble-webfont.eot?#iefix') format('embedded-opentype'),
       url('../fonts/rouble-webfont.woff2') format('woff2'),
       url('../fonts/rouble-webfont.woff') format('woff'),
       url('../fonts/rouble-webfont.ttf') format('truetype'),
       url('../fonts/rouble-webfont.svg#als_rublregular') format('svg');
  font-weight: normal;
  font-style: normal;
}
@font-face {
	font-family: 'elektra_light_proregular';
	src: url('../fonts/elektra_light_pro_regular-webfont.eot');
	src: url('../fonts/elektra_light_pro_regular-webfont.eot?#iefix') format('embedded-opentype'),
	url('../fonts/elektra_light_pro_regular-webfont.woff2') format('woff2'),
	url('../fonts/elektra_light_pro_regular-webfont.woff') format('woff'),
	url('../fonts/elektra_light_pro_regular-webfont.ttf') format('truetype');
	font-weight: normal;
	font-style: normal;
}
@font-face {
	font-family: 'pf_din_text_cond_proregular';
	src: url('../fonts/pfdinregular-webfont.eot');
	src: url('../fonts/pfdinregular-webfont.eot?#iefix') format('embedded-opentype'),
	url('../fonts/pfdinregular-webfont.woff2') format('woff2'),
	url('../fonts/pfdinregular-webfont.woff') format('woff'),
	url('../fonts/pfdinregular-webfont.ttf') format('truetype');
	font-weight: normal;
	font-style: normal;
}

// font vars

$f_elektra = 'elektra_light_proregular'
$f_pfdin = 'pf_din_text_cond_proregular'
$rub = 'als_rublregular'